    "            # MAE = []\n",
    "            # R2 = []\n",
    "            pred_list = []\n",
    "            ture_list = []\n",
    "            pred_trian_list = []\n",
    "            ture_train_list = []\n",
    "            pearson_list = []\n",
    "            RMSE_list = []\n",
